<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.xda.testbtl">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Testbtl"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".activity.OrderActivity"
            android:exported="false" />
        <activity
            android:name=".activity.ShoppingCartActivity"
            android:exported="false" />
        <activity
            android:name=".activity.ProductDetailActivity"
            android:exported="false" />
        <activity
            android:name=".activity.TopsActivity"
            android:exported="false" />
        <activity
            android:name=".activity.SizeChartActivity"
            android:exported="false" />
        <activity
            android:name=".activity.OuterwearActivity"
            android:exported="false" />
        <activity
            android:name=".activity.NewArrivalsActivity"
            android:exported="false" />
        <activity
            android:name=".activity.AboutAppActivity"
            android:exported="false" />
        <activity
            android:name=".activity.BottomsActivity"
            android:exported="false" />
        <activity
            android:name=".activity.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>